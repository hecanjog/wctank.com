<svg xmlns="http://www.w3.org/2000/svg" version="1.1">
<defs>
    
<filter id="print-analog">
    <feColorMatrix in="SourceGraphic" type="saturate" values="4.6"></feColorMatrix>
    <feComponentTransfer>
        <feFuncA type="discrete" tableValues="0.93"></feFuncA>
    </feComponentTransfer>
    <feOffset dx="2" dy="1" result="offset"></feOffset>
    
    <!-- EDGE DETECTION -->
        <feColorMatrix in="SourceGraphic" type="saturate" values="0"></feColorMatrix>
        <feGaussianBlur id="pa-denoise" stdDeviation="1.16"></feGaussianBlur>
        <feConvolveMatrix   
            kernelMatrix="-1 -1 -1 
                          -1 8 -1 
                          -1 -1 -1"
            preserveAlpha="true">
        </feConvolveMatrix>
        <feComponentTransfer result="flip">
            <feFuncR type="linear" slope="-30" intercept="1" tableValues="0 1"></feFuncR>
            <feFuncG type="linear" slope="-30" intercept="1" tableValues="0 1"></feFuncG>
            <feFuncB type="linear" slope="-30" intercept="1" tableValues="0 1"></feFuncB>
        </feComponentTransfer> 
        <feMorphology operator="erode" radius="0.001" result="thick"></feMorphology>
    <!-- /EDGE DETECTION -->
    
    <feBlend id="pa-bypass" in="offset" in2="flip" mode="multiply" result="out"></feBlend>
</filter>

<filter id="caustic-glow">
    <feColorMatrix in="SourceGraphic" type="saturate" values="0" result="gray"></feColorMatrix>
    <feComponentTransfer in="SourceGraphic">
        <feFuncR type="discrete" tableValues="0"></feFuncR>
        <feFuncG type="gamma" amplitude="2" exponent="3.2"></feFuncG>
        <feFuncB type="discrete" tableValues="0"></feFuncB>
    </feComponentTransfer>   
    <feComponentTransfer result="caustics">
        <feFuncG type="linear" slope="100" intercept="0"></feFuncG>
    </feComponentTransfer>
    <feColorMatrix in="caustics" type="luminanceToAlpha" result="alphamap"></feColorMatrix>
    <feComposite in="caustics" in2="alphamap" operator="in"></feComposite>
    <feGaussianBlur id="cg-glow-radius" stdDeviation="10.6" result="glow"></feGaussianBlur>   
    <feComposite in="gray" in2="glow" operator="xor"></feComposite>
    
    <feComponentTransfer>
        <feFuncA type="discrete" tableValues="0"></feFuncA>
    </feComponentTransfer>

    <feGaussianBlur in="alphamap" id="caustic-glow-post-blur" stdDeviation="0">
        <animate id="caustic-glow-post-blur-animate" attributeName="stdDeviation" 
            values="" calcMode="linear" dur="1000ms" repeatCount="indefinite">
        </animate>
    </feGaussianBlur> 
</filter>

<filter id="animated-blur">
    <feGaussianBlur id="animated-blur-feGaussianBlur" stdDeviation="0">
        <animate id="animated-blur-animate-node" attributeName="stdDeviation" 
            values="" calcMode="linear" dur="1000ms" repeatCount="indefinite">
        </animate>
    </feGaussianBlur> 
</filter>

<filter id="fauvist">
    <feColorMatrix id="fauvist-saturate" in="SourceGraphic" type="saturate" values="30"></feColorMatrix>
    <feGaussianBlur id="fauvist-blur" stdDeviation="1">
        <animate id="fauvist-blur-animate" attributeName="stdDeviation" 
            values="1;1.54;1" calcMode="linear" dur="1500ms" repeatCount="indefinite">
        </animate>
    </feGaussianBlur> 
</filter>

<filter id="vhs" filterRes="1">
    <feColorMatrix in="SourceGraphic" type="saturate" values="0.3" result="desaturated"></feColorMatrix>
    <feComponentTransfer in="desaturated">
        <feFuncR type="linear" slope="2.2" intercept="0"></feFuncR>
        <feFuncG type="linear" slope="2.2" intercept="0"></feFuncG>
        <feFuncB type="linear" slope="2.2" intercept="0"></feFuncB>
        <feFuncA type="discrete" tableValues="0.2"></feFuncA>
    </feComponentTransfer>
    <feGaussianBlur id="vhs-blur" stdDeviation="6" result="blurred"></feGaussianBlur>
    <feBlend in2="desaturated" in="blurred" mode="normal"></feBlend>    
    <feOffset id="vhs-offset" dx="0" dy="0"></feOffset>
</filter>

<filter id="cmgyk"> 
    <feColorMatrix in="SourceGraphic" type="saturate" values="0" result="gray"></feColorMatrix>
    <feGaussianBlur id="cmgyk-denoise" in="gray" stdDeviation="1"></feGaussianBlur>   
    <feConvolveMatrix 
        kernelMatrix="-1 -1 -1 
                      -1 8 -1 
                      -1 -1 -1" 
        preserveAlpha="true">
    </feConvolveMatrix>
    <feComponentTransfer>
        <feFuncA type="linear" slope="0.7" intercept="-0.667"></feFuncA>
    </feComponentTransfer>
    <feComponentTransfer result="edged">
        <feFuncA type="gamma" amplitude="3" exponent="0.1"></feFuncA>
    </feComponentTransfer>
    <feColorMatrix in="edged" type="luminanceToAlpha" result="alphamap"></feColorMatrix>
    <feComposite in="edged" in2="alphamap" operator="in"></feComposite> 
    <feComponentTransfer result="edged-alpha-inv">
        <feFuncA type="linear" slope="-30" intercept="1"></feFuncA>
    </feComponentTransfer>
</filter>

</defs>
</svg>
