<svg xmlns="http://www.w3.org/2000/svg" version="1.1">
<defs>

<filter id="print-analog">
	<feColorMatrix in="SourceGraphic" type="saturate" values="4.6"></feColorMatrix>
	<feComponentTransfer>
		<feFuncA type="discrete" tableValues="0.93"></feFuncA></feComponentTransfer>
	<feOffset dx="2" dy="1" result="offset"></feOffset>
	
	<!-- EDGE DETECTION -->
		<feColorMatrix in="SourceGraphic" type="saturate" values="0"></feColorMatrix>
		<feGaussianBlur id="pa-denoise" stdDeviation="1.45"></feGaussianBlur>
		<feConvolveMatrix	
			kernelMatrix="-1 -1 -1 
			              -1 8 -1 
			              -1 -1 -1"
			preserveAlpha="true"></feConvolveMatrix>
		<feComponentTransfer result="flip">
			<feFuncR type="linear" slope="-30" intercept="1" tableValues="0 1"></feFuncR>
			<feFuncG type="linear" slope="-30" intercept="1" tableValues="0 1"></feFuncG>
			<feFuncB type="linear" slope="-30" intercept="1" tableValues="0 1"></feFuncB></feComponentTransfer>	
		<feMorphology operator="erode" radius="0.001" result="thick"></feMorphology>
	<!-- /EDGE DETECTION -->
	
	<feBlend id="pa-bypass" in="offset" in2="thick" mode="multiply" result="out"></feBlend>
</filter>

<filter id="caustic-glow">
	<feColorMatrix in="SourceGraphic" type="saturate" values="0" result="gray"></feColorMatrix>
	
	<feComponentTransfer in="SourceGraphic">
	<feFuncR type="discrete" tableValues="0"></feFuncR>
		<feFuncG type="gamma" amplitude="2" exponent="5"></feFuncG>
		<feFuncB type="discrete" tableValues="0"></feFuncB></feComponentTransfer>	
	<feComponentTransfer result="caustics">
		<feFuncG type="linear" slope="100" intercept="0"></feFuncG></feComponentTransfer>
	<feColorMatrix in="caustics" type="luminanceToAlpha" result="alphamap"></feColorMatrix>
	<feComposite in="caustics" in2="alphamap" operator="in"></feComposite>
	<feGaussianBlur id="cg-glow-radius" stdDeviation="12" result="glow"></feGaussianBlur>	
	
	<feComposite in="gray" in2="glow" operator="xor"></feComposite>
</filter>

<filter id="fauvist">
	<!-- Subtract red dependent on zoom level -->
	<!-- multiply in new perlin noise on drag or zoom events -->
	<feColorMatrix id="fauvist-saturate" in="SourceGraphic" type="saturate" values="20"></feColorMatrix>
	<!-- try offsetting color channels  -->	
</filter>

<filter id="vhs" filterRes="1">
	<feColorMatrix in="SourceGraphic" type="saturate" values="0.3" result="desaturated"></feColorMatrix>
	<feTurbulence id="vhs-noise" type="fractalNoise" baseFrequency="0.25" numOctaves="10" seed="1" result="noise"></feTurbulence>
	<feBlend in="noise" in2="desaturated" mode="multiply"></feBlend>
	<feOffset id="vhs-offset" dx="0" dy="0"></feOffset>
</filter>

<filter id="spectroscopy">
	<!-- rm green, offset red and blue channels... on init, force apart, slowly bring closer -->
</filter>

<filter id="projection-blur">
	<!-- pixelwise offset of r, g, b channels -->
</filter>

<!-- the fill property of the target div should be set to this gradient  -->
<linearGradient id="cmgyk-rainbow" x1="0" y1="0" x2="100%" y2="100%" gradientTransform="rotate(0)">
		<stop offset="0%" stop-color="#FFFF00"></stop>
		<stop offset="9%" stop-color="#00FF00"></stop>
		<stop offset="18%" stop-color="#00FFFF"></stop>
		<stop offset="27%" stop-color="#6F00FF"></stop>
		<stop offset="36%" stop-color="#FF00FF"></stop>
		<stop offset="45%" stop-color="#FF0000"></stop>
		<stop offset="54%" stop-color="#FF7F00"></stop>
		<stop offset="63%" stop-color="#FFFF00"></stop>
		<stop offset="72%" stop-color="#00FF00"></stop>
		<stop offset="81%" stop-color="#00FFFF"></stop>
		<stop offset="90%" stop-color="#6F00FF"></stop>
		<stop offset="99%" stop-color="#FF00FF"></stop></linearGradient>

<filter id="cmgyk">	
	<!-- EDGE DETECTION -->
		<feColorMatrix in="SourceGraphic" type="saturate" values="0" result="gray"></feColorMatrix>
		
		<feGaussianBlur id="cmgyk-denoise" in="gray" stdDeviation="1.25"></feGaussianBlur>	
		<feConvolveMatrix kernelMatrix="-1 -1 -1 -1 8 -1 -1 -1 -1" preserveAlpha="true"></feConvolveMatrix>
		<feComponentTransfer>
			<feFuncA type="linear" slope="0.7" intercept="-0.64"></feFuncA></feComponentTransfer>
  		<feComponentTransfer result="edged">
			<feFuncA type="gamma" amplitude="23" exponent="2.313"></feFuncA></feComponentTransfer>
		<feColorMatrix in="edged" type="luminanceToAlpha" result="alphamap"></feColorMatrix>
		<feComposite in="edged" in2="alphamap" operator="in"></feComposite> 
		<feComponentTransfer result="edged-alpha-inv">
			<feFuncA type="linear" slope="-30" intercept="1"></feFuncA></feComponentTransfer>
	<!-- /EDGE DETECTION -->

	<feColorMatrix id="cmgyk-hueRotate" in="FillPaint" type="hueRotate" values="0" result="rotgrad"></feColorMatrix>
	<feComposite in="edged-alpha-inv" in2="rotgrad" operator="atop" result="rainbow-edges"></feComposite>	
</filter>

