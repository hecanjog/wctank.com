<svg xmlns="http://www.w3.org/2000/svg" version="1.1">
<defs>

<filter id="print-analog">
	<feColorMatrix in="SourceGraphic" type="saturate" values="4.6"></feColorMatrix>
	<feComponentTransfer>
		<feFuncA type="discrete" tableValues="0.93"></feFuncA></feComponentTransfer>
	<feOffset dx="2" dy="1" result="offset"></feOffset>
	
	<!-- EDGE DETECTION -->
		<feColorMatrix in="SourceGraphic" type="saturate" values="0"></feColorMatrix>
		<feGaussianBlur id="pablur" stdDeviation="1.45"></feGaussianBlur>
		<feConvolveMatrix	
			kernelMatrix="-1 -1 -1 
			              -1 8 -1 
			              -1 -1 -1"
			preserveAlpha="true"></feConvolveMatrix>
		<feComponentTransfer result="flip">
			<feFuncR type="linear" slope="-30" intercept="1" tableValues="0 1"></feFuncR>
			<feFuncG type="linear" slope="-30" intercept="1" tableValues="0 1"></feFuncG>
			<feFuncB type="linear" slope="-30" intercept="1" tableValues="0 1"></feFuncB></feComponentTransfer>	
		<feMorphology operator="erode" radius="0.001" result="thick"></feMorphology>
	<!-- /EDGE DETECTION -->
	
	<feBlend id="pablend" in="offset" in2="thick" mode="multiply" result="out"></feBlend>
</filter>

<filter id="caustic-glow">
	<feColorMatrix in="SourceGraphic" type="saturate" values="0" result="gray"></feColorMatrix>
	
	<feComponentTransfer in="SourceGraphic">
		<feFuncR type="discrete" tableValues="0"></feFuncR>
		<feFuncG type="gamma" amplitude="2" exponent="5"></feFuncG>
		<feFuncB type="discrete" tableValues="0"></feFuncB></feComponentTransfer>	
	<feComponentTransfer result="caustics">
		<feFuncG type="linear" slope="100" intercept="0"></feFuncG></feComponentTransfer>
	<feColorMatrix in="caustics" type="luminanceToAlpha" result="alphamap"></feColorMatrix>
	<feComposite in="caustics" in2="alphamap" operator="in"></feComposite>
	<feGaussianBlur id="cgblur" stdDeviation="12" result="glow"></feGaussianBlur>	
	
	<feComposite in="gray" in2="glow" operator="xor"></feComposite>
</filter>

<filter id="fauvist">
	<!-- Subtract red dependent on zoom level -->
	<!-- multiply in new perlin noise on drag or zoom events -->
	<feColorMatrix in="SourceGraphic" type="saturate" values="50" result="wtf"></feColorMatrix>
	
</filter>

<filter id="VHS">
	<!-- desaturate, blend with seed-animated noise (video frame rate), very fast (>fr) vertical offset jitter on idle -->

</filter>

<filter id="spectroscopy">
	<!-- rm green, offset red and blue channels... on init, force apart, slowly bring closer -->
</filter>
<filter id="projection-blur">
	<!-- pixelwise offset of r, g, b channels -->
</filter>

<filter id="CMGYK">
	<!-- EDGE DETECTION -->
		<feColorMatrix in="SourceGraphic" type="saturate" values="0"></feColorMatrix>
		<feConvolveMatrix kernelMatrix="-1 -1 -1 -1 8 -1 -1 -1 -1" preserveAlpha="true" result="edged"></feConvolveMatrix>
		<feColorMatrix in="edged" type="luminanceToAlpha" result="alphamap"></feColorMatrix>
		<feComposite in="edged" in2="alphamap" operator="in"></feComposite> 
		<feComponentTransfer result="edgealphinv">
			<feFuncA type="linear" slope="-30" intercept="1"></feFuncA></feComponentTransfer>
	<!-- /EDGE DETECTION -->
	
	<feFlood id="cflood" flood-color="#FFFF00"></feFlood>
	<feColorMatrix id="cmatrix" type="hueRotate" values="0" result="rotflood"></feColorMatrix> 
	<feComposite in="edgealphinv" in2="rotflood" operator="atop" result="out"></feComposite>
</filter>

<filter id="eriks-purple-scat-fantasy">
	<!-- whenever something is brown->purple, if purple->erik -->
</filter>

<filter id="birds">
	<!-- separate colors, replace each with a tiled picture of a bird -->
</filter>

<filter id="jason">
	<!-- like birds but with jason -->
</filter>

</defs>
</svg>
