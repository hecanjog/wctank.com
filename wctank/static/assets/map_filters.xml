<svg xmlns="http://www.w3.org/2000/svg" version="1.1">
<defs>

<filter id="print-analog">
    <feColorMatrix in="SourceGraphic" type="saturate" values="4.6"></feColorMatrix>
    <feComponentTransfer>
        <feFuncA type="discrete" tableValues="0.93"></feFuncA>
    </feComponentTransfer>
    <feOffset dx="2" dy="1" result="offset"></feOffset>
    
    <!-- EDGE DETECTION -->
        <feColorMatrix in="SourceGraphic" type="saturate" values="0"></feColorMatrix>
        <feGaussianBlur id="pa-denoise" stdDeviation="1.16"></feGaussianBlur>
        <feConvolveMatrix   
            kernelMatrix="-1 -1 -1 
                          -1 8 -1 
                          -1 -1 -1"
            preserveAlpha="true">
        </feConvolveMatrix>
        <feComponentTransfer result="flip">
            <feFuncR type="linear" slope="-30" intercept="1" tableValues="0 1"></feFuncR>
            <feFuncG type="linear" slope="-30" intercept="1" tableValues="0 1"></feFuncG>
            <feFuncB type="linear" slope="-30" intercept="1" tableValues="0 1"></feFuncB>
        </feComponentTransfer> 
        <feMorphology operator="erode" radius="0.001" result="thick"></feMorphology>
    <!-- /EDGE DETECTION -->
    
    <feBlend id="pa-bypass" in="offset" in2="flip" mode="multiply" result="out"></feBlend>
    <feComponentTransfer>
        <feFuncA type="discrete" tableValues="0.75"></feFuncA>
    </feComponentTransfer> 

</filter>

<filter id="animated-blur">
    <feGaussianBlur id="animated-blur-feGaussianBlur" stdDeviation="0">
        <animate id="animated-blur-animate-node" attributeName="stdDeviation" 
            values="" calcMode="linear" dur="1000ms" repeatCount="indefinite">
        </animate>
    </feGaussianBlur> 
</filter>

</defs>
</svg>
